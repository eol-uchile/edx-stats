{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ProcessVisits",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "processvisits",
        "2019-01-01"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Django Test",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "test"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Run Django",
      "type": "python",
      "request": "attach",
      "pathMappings": [
        {
          "localRoot": "/app",
          "remoteRoot": "/app"
        }
      ],
      "port": 3500,
      "host": "127.0.0.1",
      "justMyCode": false,
    }
  ]
}